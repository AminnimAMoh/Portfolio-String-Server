{
  "presets": [
    [
      "@babel/env",
      {
        "useBuiltIns": "entry",
        "targets": {
          "browsers": ">1%, not ie 11, not op_mini all",
          "node": 12
        }
      }
    ],
    "@babel/react",
    "@babel/typescript"
  ],
  "plugins": [
    "@babel/proposal-json-strings",
    "@babel/syntax-dynamic-import",
    "@babel/transform-runtime",
    [
      "import",
      {
        "libraryName": "@material-ui/core",
        "libraryDirectory": "",
        "camel2DashComponentName": false
      }
    ]
  ]
}
